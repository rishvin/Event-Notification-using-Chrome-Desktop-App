{
  "name": "RJ Notification",
  "description": "Notification app.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["unlimitedStorage", "storage", "alarms", "notifications", "webview", "<all_urls>", { "fileSystem": ["write"] } ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "web_accessible_resources": [
    "images/*"
    ]
}